<?xml version="1.0"?>

<launch>

    <master auto="start"/>
    <param name="/use_sim_time" value="true"/>

    <node pkg="gmapping" type="slam_gmapping" name="slam_gmapping">
        <remap from="odom" to="/odom" />
        <param name="scan" value="/scan" />
        <param name="base_frame" value="chassis" />
        <param name="odom_frame" value="odom" />
        <param name="map_frame" value="map" />
        <param name="map_update_interval" value="0.3" />
        <param name="maxUrange" value="8.0" />
        <param name="srr" value="0.0" /> <!--noise to the map-->
        <param name="srt" value="0.0" /> <!--noise to the map-->
        <param name="str" value="0.0" /> <!--noise to the map-->
        <param name="stt" value="0.0" /> <!--noise to the map-->
        <param name="minimumScore" value="1200" />
        <param name="linearUpdate" value="0.1" />
        <param name="angularUpdate" value="0.1" />
        <param name="xmin" value="-5.0" />
        <param name="ymin" value="-5.0" />
        <param name="xmax" value="5.0" />
        <param name="ymax" value="5.0" />
        <param name="delta" value="0.04" />
    </node>
    
</launch>
